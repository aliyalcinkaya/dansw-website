# Eventbrite API Configuration
# Copy this file to .env and fill in your actual values
#
# Frontend value (safe to expose): organization ID only
VITE_EVENTBRITE_ORGANIZATION_ID=your_organization_id_here
# Optional: Supabase Edge Function name used for Eventbrite proxy/sync
VITE_SUPABASE_EVENTBRITE_FUNCTION=dynamic-worker
# Optional: full Eventbrite function URL override (if function route/name differs)
# Example: https://your-project-ref.supabase.co/functions/v1/dynamic-worker
VITE_SUPABASE_EVENTBRITE_FUNCTION_URL=

# Edge Function secrets (NOT exposed in client bundle)
# Use these for Supabase Edge Functions (set as Supabase secrets in production)
EVENTBRITE_PRIVATE_TOKEN=your_eventbrite_private_token_here
EVENTBRITE_ORGANIZATION_ID=your_organization_id_here
# Comma/newline-separated origins. Example:
# EVENTBRITE_ALLOWED_ORIGIN=http://localhost:5173,https://www.dawsydney.org.au
EVENTBRITE_ALLOWED_ORIGIN=http://localhost:5173

# LinkedIn embeds (optional)
# Comma or newline-separated LinkedIn post URLs or urn:li links.
# Example:
# VITE_LINKEDIN_POST_URLS=https://www.linkedin.com/feed/update/urn:li:activity:123...,https://www.linkedin.com/posts/your-page_activity-456...
VITE_LINKEDIN_POST_URLS=

# Supabase form backend (required for website forms)
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_SUPABASE_FORMS_TABLE=form_submissions
VITE_SUPABASE_NEWSLETTER_TABLE=newsletter_subscriptions
# Optional: Supabase Edge Function name to mirror newsletter signups to Mailchimp
VITE_SUPABASE_NEWSLETTER_MAILCHIMP_FUNCTION=newsletter-mailchimp-sync
# Optional: Supabase Edge Function used for form/job email forwarding
VITE_SUPABASE_FORM_FORWARDER_FUNCTION=form-forwarder
# Optional: full forwarding function URL override
# Example: https://your-project-ref.supabase.co/functions/v1/form-forwarder
VITE_SUPABASE_FORM_FORWARDER_FUNCTION_URL=
# Optional: admin routing table override
VITE_SUPABASE_FORM_ROUTING_TABLE=form_email_routing
VITE_SUPABASE_JOBS_TABLE=job_posts
VITE_SUPABASE_JOB_APPLICATIONS_TABLE=job_applications
VITE_SUPABASE_JOB_ADMIN_NOTIFICATIONS_TABLE=job_admin_notifications
VITE_SUPABASE_SITE_SETTINGS_TABLE=site_settings

# Job board payments (optional)
# Preferred: backend endpoint that creates Stripe Checkout sessions
VITE_STRIPE_CHECKOUT_ENDPOINT=https://your-api.example.com/create-job-checkout
# Fallback: direct Stripe Payment Links for each package
VITE_STRIPE_STANDARD_PAYMENT_LINK=https://buy.stripe.com/your_standard_link
VITE_STRIPE_AMPLIFIED_PAYMENT_LINK=https://buy.stripe.com/your_amplified_link
# Temporary test mode: when true, publish skips Stripe and goes live immediately
VITE_JOBS_DISABLE_PAYMENTS=false

# Optional Brandfetch API key for auto logo/color enrichment on jobs
VITE_BRANDFETCH_API_KEY=your_brandfetch_api_key_here

# Optional analytics for form submissions
VITE_MIXPANEL_TOKEN=your_mixpanel_project_token_here
